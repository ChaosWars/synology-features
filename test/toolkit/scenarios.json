{
    "toolkit": {
        "containerEnv": {
            "ARCHITECTURE": "geminilake",
            "DSMVERSION": "7.2",
            "PKGSCRIPTSDIR": "pkgscripts-ng",
            "PKGSCRIPTSPATH": "/toolkit/pkgscripts-ng",
            "TOOLKITPATH": "/toolkit"
        },
        "features": {
            "toolkit": {
                "dsmVersion": "7.2",
                "architecture": "geminilake"
            }
        },
        "image": "mcr.microsoft.com/devcontainers/base:1-jammy",
        "mounts": [
            {
                "source": "toolkit",
                "target": "/toolkit",
                "type": "volume"
            }
        ],
        "remoteUser": "root"
    }
}
